{
    "name": "kyleatdnd/gridpane-api-client-php",
    "type": "library",
    "description": "GridPane API PHP SDK",
    "minimum-stability": "alpha",
    "keywords": [
        "gridpane"
    ],
    "homepage": "http://designanddevelop.io",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleAtDND",
            "email": "Wesley@DesignAndDevelop.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0",
        "php": ">=8.1",
        "doctrine/inflector": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "@stable",
        "fzaninotto/faker": ">=1.5.0",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "GridPane\\Api\\": "src/api",
            "GridPane\\Console\\": "src/console"
        }
    }
}
